{
    "name": "template_engine",
    "version": "0.0.1",
    "package": "template_engine",
    "server": {
        "proxies": {
            "/search": "http://api.host:8080/search",
            "/api2": "http://api.host:8080/"
        },
        "routers": {
            "/src/*": "./src/{0}",
            "/*.json": {
                "path": "./testdatas/{0}.json",
                "template": true
            }
        },
        "port": 9800
    }
}
